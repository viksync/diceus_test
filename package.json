{
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "npm run typecheck && npm run build:bundle",
    "build:bundle": "esbuild src/main.ts --bundle --platform=node --format=esm --outfile=build/main.js --packages=external --log-level=error",
    "typecheck": "tsc --noEmit",
    "start": "node build/main.js",
    "dev": "tsx watch src/main.ts",
    "clean": "rm -rf build"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "24.2.1",
    "@types/pdfkit": "^0.17.3",
    "esbuild": "^0.25.11",
    "tsx": "^4.20.6",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@openai/agents": "^0.1.10",
    "colorette": "^2.0.20",
    "express": "^5.1.0",
    "mindee": "^4.32.0",
    "morgan": "^1.10.1",
    "pdfkit": "^0.17.2",
    "zod": "^3.25.76"
  }
}
